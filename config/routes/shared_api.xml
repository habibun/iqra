<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="shared_register" path="/sign-up"
           controller="App\Shared\Presentation\Controller\UserController::signUp"/>

    <route id="shared_sign_in" path="/sign-in"
           controller="App\Shared\Presentation\Controller\UserController::signIn"/>
</routes>
